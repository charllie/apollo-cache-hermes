// Place your settings in this file to overwrite default and user settings.
{
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/*.js.map": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "{src,test}/**/*.d.ts": true,
    "{src,test}/**/*.js": true,
    "node_modules/": true,
    "output/": true
  },
  "cSpell.words": ["queryable"]
}
